{
  "name": "alfred-portfolio-bot",
  "version": "1.0.0",
  "description": "AI Assistant Alfred for Portfolio - Powered by Groq",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:css": "tailwindcss -i ./src/styles/input.css -o ./public/css/styles.css --watch",
    "build": "npm run build:css && npm run build:widget",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./public/css/styles.css --minify",
    "build:widget": "echo 'Widget built successfully'",
    "vercel-build": "npm run build"
  },
  "keywords": ["ai", "chatbot", "portfolio", "groq", "assistant"],
  "author": "Jeeva",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "groq-sdk": "^0.7.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  }
}
